<h1>Sessions#new</h1>

<%= form_tag login_path, method: 'post' do %>
	<div class="form-group">
		<%= label_tag :Email %>
		<%= text_field_tag :email , "" , class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= label_tag :password %>
		<%= password_field_tag :password , "" , class: 'form-control' %>
	</div>

	<%= submit_tag 'Sign In' %>

<% end%>
